version: "3.5"

services:

  influxdb-meta-01:
    image: chengshiwen/influxdb:1.8.10-c1.1.2-meta
    container_name: influxdb-meta-01
    hostname: influxdb-meta-01
    restart: unless-stopped
    ports:
      - 8091:8091
    volumes:
      - ./etc/influxdb:/etc/influxdb
    networks:
      - influxdb-cluster

networks:
  influxdb-cluster:
    driver: bridge
