version: "3.5"

services:

  influxdb-meta-02:
    image: chengshiwen/influxdb:1.8.10-c1.1.2-meta
    container_name: influxdb-meta-02
    hostname: influxdb-meta-02
    restart: unless-stopped
    ports:
      - 8091:8091
    volumes:
      - ./influxdb-meta-02.conf:/etc/influxdb/influxdb-meta.conf
    networks:
      - influxdb-cluster

networks:
  influxdb-cluster:
    driver: bridge
