version: "3.5"

services:

  influxdb-data-02:
    image: chengshiwen/influxdb:1.8.10-c1.1.2-data
    container_name: influxdb-data-02
    hostname: influxdb-data-02
    ports:
      - 8086:8086
    restart: unless-stopped
    volumes:
      - ./influxdb-data-02.conf:/etc/influxdb/influxdb.conf
    networks:
      - influxdb-cluster

networks:
  influxdb-cluster:
    driver: bridge
